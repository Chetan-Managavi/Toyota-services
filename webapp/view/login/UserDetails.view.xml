<mvc:View
	controllerName="ui5.walkthrough.controller.UserDetails"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
<Page id="UserDetails" showNavButton="true" title="User Profile"    >

	


		
	

	<Panel headerText="{userdata>username}"   >
		<content>
			<Text text="{userdata>id}" ></Text>
			<Text text="{userdata>name}"></Text>
		</content>
	</Panel>


</Page>
</mvc:View>